{
  "build": {
    "sitemap": {
        "baseUrl": "https://docs.nav-x.com/en/"
    },
    "content": [
      {
        "files": [
            "locale.md",
            "index.md",
            "toc.yml"
        ],
        "exclude": [
            ".gitignore",
            "**/obj/**",
            "obj/**",
            "README.md",
            "**/_exported_templates/**"
        ]
      },
      {
          "files": [
              "business-central/**.yml",
              "business-central/**.md",
              "business-central/toc.yml"
          ]
      },
      {
          "files": [
              "business-central/commissions/**.md",
              "business-central/credit-management/**.md",
              "business-central/credit-card/**.md",
              "business-central/national-accounts/**.md",
              "business-central/productivity/**.md",
              "business-central/search/**.md"
          ],
          "exclude": [
              "business-central/commissions/toc.yml",
              "business-central/credit-management/toc.yml",
              "business-central/credit-card/toc.yml",
              "business-central/custom-fields/toc.yml",
              "business-central/integration-framework/toc.yml",
              "business-central/national-accounts/toc.yml",
              "business-central/opsuite/toc.yml",
              "business-central/productivity/toc.yml",
              "business-central/search/toc.yml",
              "business-central/warehouse/toc.yml"
          ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/images/**",
          "web.config"
        ]
      }
    ],
    "dest": "../_site",
    "globalMetadata": {
        "ROBOTS": "INDEX, FOLLOW",
        "_appLogoPath": "/images/NAV-X-logo.png",
        "_appFaviconPath": "/images/navx.ico",
        "_appTitle": "NAV-X Docs",
        "_appFooter": "Copyright &copy; 2022-2025 by NAV-X LLC",
        "_enableSearch": true,
        "_enableNewTab": false,
        "_disableContribution": true,
        "_basePath": "https://docs.nav-x.com/",
        "_langCode": "en",
        "_langName": "English"
    },
    "fileMetadataFiles": [],
    "template": [
        "../templates/navx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "force": true
  }
}